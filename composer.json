{
  "name": "grinway/service-bundle",
  "type": "symfony-bundle",
  "version": "1.7.0",
  "description": "Commonly used tools for a symfony app",
  "keywords": [
    "grinway",
    "symfony",
    "service-bundle",
    "symfony-services",
    "bundle",
    "symfony-ux"
  ],
  "license": "MIT",
  "require-dev": {
    "symfony/phpunit-bridge": "^7.2|^6",
    "phpunit/phpunit": "11.4",
    "symfony/test-pack": "^1.0",
    "symfony/maker-bundle": "^1.62",
    "zenstruck/foundry": "^2.3"
  },
  "authors": [
    {
      "name": "Grigory Koblitskiy",
      "email": "grin180898@outlook.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "GrinWay\\Service\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GrinWay\\Service\\Tests\\": "tests/"
    }
  },
  "conflict": {
    "symfony/flex": "<1.13"
  },
  "extra": {},
  "minimum-stability": "stable",
  "require": {
    "symfony/framework-bundle": "^7.2|^6",
    "symfony/runtime": "^7.2|^6",
    "symfony/http-client": "^7.2|^6",
    "symfony/console": "^7.2|^6",
    "symfony/translation": "^7.2|^6",
    "symfony/property-access": "^7.2|^6",
    "symfony/messenger": "^7.2|^6",
    "symfony/serializer": "^7.2|^6",
    "symfony/notifier": "^7.2|^6",
    "symfony/string": "^7.2|^6",
    "symfony/stimulus-bundle": "^2.22",
    "symfony/webpack-encore-bundle": "^2.2",
    "endroid/installer": "^1",
    "symfony/yaml": "^7.2|^6",
    "symfony/validator": "^7.2|^6",
    "symfony/cache": "^7.2|^6",
    "nesbot/carbon": "^3.8",
    "symfony/process": "^7.2|^6",
    "doctrine/doctrine-bundle": "^2.13",
    "symfony/dotenv": "^7.2|^6",
    "symfony/filesystem": "^7.2",
    "symfony/orm-pack": "^2.4",
    "doctrine/orm": "^3.3",
    "doctrine/dbal": "^3.9",
    "doctrine/doctrine-fixtures-bundle": "^4.0",
    "symfony/asset": "^7.2"
  },
  "config": {
    "allow-plugins": {
      "symfony/runtime": true,
      "endroid/installer": true
    }
  }
}
